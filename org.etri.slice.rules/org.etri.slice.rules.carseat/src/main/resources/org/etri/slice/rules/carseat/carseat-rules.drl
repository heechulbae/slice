package org.etri.slice.rules.carseat;

import org.etri.slice.commons.car.context.SeatPosture;
import org.etri.slice.commons.car.event.SeatPostureChanged;

rule "SeatPosture Change Notification"
	when
		$e: SeatPosture()
	then
		channels["car/seat_posture_changed"].send(new SeatPostureChanged($e));
end
