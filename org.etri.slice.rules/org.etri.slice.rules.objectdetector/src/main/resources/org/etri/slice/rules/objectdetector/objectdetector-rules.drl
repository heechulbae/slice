package org.etri.slice.rules.objectdetector;

import org.etri.slice.commons.car.context.ObjectInfo;
import org.etri.slice.commons.car.event.ObjectDetected;


rule "Object Detection Notification"
	when 
	   $info: ObjectInfo()
	then 
	   channels["car/object_detected"].send(new ObjectDetected($info));
end
